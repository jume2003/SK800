<UserControl
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:SK_ABO.UserCtrls"
             xmlns:ed="http://schemas.microsoft.com/expression/2010/drawing" x:Class="SK_ABO.UserCtrls.Barcode_Control"
             mc:Ignorable="d" 
             d:DesignHeight="50" d:DesignWidth="50">
    <UserControl.Resources>
        <Style TargetType="Label">
            <Setter Property="FontFamily" Value="Font Awesome 5 Free Solid"/>
            <Setter Property="FontSize" Value="19"/>
        </Style>
    </UserControl.Resources>
    <Grid>
        
        <Rectangle Fill="#FFF4F4F5" HorizontalAlignment="Stretch" Margin="0.0,0,0,0" Stroke="Black" VerticalAlignment="Stretch" />
        <Label Content="&#xF02A;&#xF02A;" HorizontalAlignment="Left" Margin="0,0,0,0" VerticalAlignment="Top"  />
        <Label Content="&#xF02A;&#xF02A;" HorizontalAlignment="Left" Margin="0,20,0,0" VerticalAlignment="top"/>
        <Line X1="0" Y1="25" X2="50" Y2="25" Stroke="Red" StrokeThickness ="3">
            
        </Line>
    </Grid>
</UserControl>
